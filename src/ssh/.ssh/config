Host mango-devops
  Hostname 43.229.63.155
  User mango
   # Forward SSH agent
  ForwardAgent yes
  # Forward GPG agent
  # See the following resources:
  # https://stackoverflow.com/questions/30058030/how-to-use-gpg-signing-key-on-a-remote-server
  # https://bigeagle.me/2016/07/GPG-and-SSH-agent-forwarding/
  # Also set StreamLocalBindUnlink yes in server's sshd_config if possible.
  #
  # Don't forget to import your PUBLIC key to the remote server user's keyring.
  # Then you should be able to encrypt/decrypt as normal.
  StreamLocalBindUnlink yes
  RemoteForward /run/user/1002/gnupg/S.gpg-agent /Users/rkm/.gnupg/S.gpg-agent

