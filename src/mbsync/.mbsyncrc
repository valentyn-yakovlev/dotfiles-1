# ~/.mbsyncrc

Expunge         Both
Create          Both
Sync            All
SyncState       *

# eqyiel@gmail.com

IMAPAccount     gmail
Host            imap.gmail.com
User            eqyiel@gmail.com
PassCmd         "pass auth-sources/eqyiel@imap.gmail.com | head -n 1"
SSLType         IMAPS
SSLVersions     TLSv1.2
CertificateFile /etc/ssl/certs/ca-certificates.crt

IMAPStore       gmail-remote
Account         gmail

MaildirStore    gmail-local
Path            ~/mail/gmail.com/
Inbox           ~/mail/gmail.com/

Channel         g_all_mail
Master          ":gmail-remote:[Gmail]/All Mail"
Slave           ":gmail-local:New"

Channel         g_sent
Master          ":gmail-remote:[Gmail]/Sent Mail"
Slave           ":gmail-local:Sent"

Channel         g_bin
Master          ":gmail-remote:[Gmail]/Bin"
Slave           ":gmail-local:Bin"

Channel         g_spam
Master          ":gmail-remote:[Gmail]/Spam"
Slave           ":gmail-local:Spam"

# mahe0054@flinders.edu.au

IMAPAccount     flinders
Host            outlook.office365.com
User            mahe0054@uni.flinders.edu.au
PassCmd         "pass auth-sources/mahe0054@outlook.office365.com | head -n 1"
Port            993
SSLType         IMAPS
SSLVersions     TLSv1.2
CertificateFile /etc/ssl/certs/ca-certificates.crt

IMAPStore       flinders-remote
