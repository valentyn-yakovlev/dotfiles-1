xkb_keymap {
  xkb_keycodes { include "evdev+aliases(qwerty)" };

  xkb_types { include "complete" };

  xkb_compatibility { include "complete" };

  xkb_symbols {
    include "pc+us+us:2+inet(evdev)"
    key <PRSC> { [Multi_key] };
    key <CAPS> { [Hyper_L] };
    modifier_map Mod3 { <CAPS> };
  };

  xkb_geometry { include "pc(pc104)" };
};
