# ~/.msmtprc

defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
syslog         on

# maher.fyi
account        ruben
host           maher.fyi
port           587
from           ruben@maher.fyi
user           ruben@maher.fyi
passwordeval   "pass auth-sources/ruben@maher.fyi | head -n 1"

